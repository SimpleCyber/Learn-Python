    "### Working with missing values"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "array([1., 2., 3., 4., 6.])"
      ]
     },
     "execution_count": 5,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "# np.nan  => null value (missing value)\n",
    "\n",
    "# nan is different from none null\n",
    "\n",
    "a = np.array([1,2,3,4,np.nan,6])\n",
    "\n",
    "a\n",
    "\n",
    "# #noheadache , remove the missing values\n",
    "\n",
    "# boolean indexing\n",
    "\n",
    "a[~(np.isnan(a))]\n"
   ]
  }
 ],
